@page
@{ Layout = "../shared/_layout"; }

<el-form size="small" ref="form" :model="form" label-width="120px">
  <el-form-item label="方向">
    <el-radio-group v-model="form.isAdd">
      <el-radio-button :label="true">增加</el-radio-button>
      <el-radio-button :label="false">减少</el-radio-button>
    </el-radio-group>
  </el-form-item>
  <el-form-item label="点击量" prop="hits" :rules="{ required: true, message: '请输入点击量' }">
    <el-input v-model="form.hits"></el-input>
  </el-form-item>
</el-form>

<div style="height: 10px"></div>

<el-divider></el-divider>
<el-row align="right" style="margin-right: 5px; margin-top: 10px;">
  <el-col :span="24" align="right">
    <el-button size="small" type="primary" v-on:click="btnSubmitClick">确 定</el-button>
    <el-button size="small" v-on:click="btnCancelClick">取 消</el-button>
  </el-col>
</el-row>

@section Scripts{
<script src="~/assets/js/cms/contentsLayerHits.js" type="text/javascript"></script> }